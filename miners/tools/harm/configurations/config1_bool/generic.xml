<harm>
	<context name="without_next">

		<prop exp="req1" loc="dt"/>
        <prop exp="req1 && !req2" loc="dt"/>
		<prop exp="req2" loc="dt"/>

		<prop exp="gnt1" loc="c"/>
		<prop exp="gnt2" loc="c"/>



		<template dtLimits="2A,0E,O" exp="G({..&&..}|-> P0)" /> 



		<sort name="causality" exp="1-afct/traceLength"/>
		<sort name="frequency" exp="atct/traceLength"/>
	</context>

	<context name="with_next">

		<prop exp="req1" loc="dt"/>
		<prop exp="req2" loc="dt"/>

		<prop exp="gnt1" loc="c"/>
		<prop exp="gnt2" loc="c"/>



		<template dtLimits="2A,0E,O" exp="G({..##1..}|-> P0)" /> 



		<sort name="causality" exp="1-afct/traceLength"/>
		<sort name="frequency" exp="atct/traceLength"/>
	</context>
</harm>



